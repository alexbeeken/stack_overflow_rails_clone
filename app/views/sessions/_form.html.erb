 <%= render 'layouts/errors', :object => User.new %>
 <div class="login_form">
 <h2>Log In</h2>
 <%= form_tag log_in_path, class: "form" do %>
   <%= label_tag :email %>
   <%= text_field_tag :email%>
   <br>
   <%= label_tag :password %>
   <%= password_field_tag :password %>
   <br>
   <br>
   <%= submit_tag "Log in" %>
 <% end %>
</div>
